{
	"extends": [
	  "config:recommended"
	],
	"vulnerabilityAlerts": {
	  "enabled": true
	},
	"osvVulnerabilityAlerts": true,
	"packageRules": [
	  {
		"enabled": false,
		"matchPackageNames": [
		  "*"
		]
	  },
	  {
		"matchManagers": [
		  "gomod"
		],
		"matchUpdateTypes": [
		  "major",
		  "minor",
		  "patch"
		],
		"enabled": false
	  }
	],
	"gomod": {
	  "enabled": true
	},
	"postUpdateOptions": [
	  "gomodTidy"
	],
	"postUpgradeTasks": {
	  "commands": [
		"go mod tidy"
	  ],
	  "fileFilters": [
		"go.mod",
		"go.sum"
	  ]
	}
  }